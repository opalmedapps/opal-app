{
  "branches": ["QSCCD-946.SB.semantic-release"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    [
      "@semantic-release/exec", {
        "analyzeCommitsCmd": "export VERSION=nextRelease.version",
        "prepareCmd": "node -e \"require('./opal_env.setup.js').setVersionAndBuild($VERSION, '1', $ENV)\""
      }
    ],
    [
      "@semantic-release/git", {
        "assets": ["CHANGELOG.md", "./env/*/config.xml"]
      }
    ]
  ],
  "dryRun": false,
  "tagFormat": "test${version}"
}
